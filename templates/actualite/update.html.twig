{% extends 'baseadmin.html.twig'%}
{% block title  %}Les modifier actualite{% endblock %}
{%block body %}
    <div style="position: absolute; margin-left: 500px; margin-top: 100px">
<h4>update actualite</h4>
        {{form_start(form,{'attr':{'novalidate':'novalidate'}})}}
        <table>
            <tr>
                <td> {{form_label(form.titre,"Titre")}}</td>
                <td> {{form_widget(form.titre)}}</td>
                <td> {{form_errors(form.titre)}}</td>
            </tr>
            <tr>
                <td> {{form_label(form.image,"image")}}</td>
                <td> {{form_widget(form.image)}}</td>
                <td> {{form_errors(form.image)}}</td>
            </tr>
            <tr>
                <td> {{form_label(form.description,"Description")}}</td>
                <td> {{form_widget(form.description)}}</td>
                <td> {{form_errors(form.description)}}</td>
            </tr>
            <tr>
                <td> {{form_label(form.categorie,"Categorie")}}</td>
                <td> {{form_widget(form.categorie)}}</td>
                <td> {{form_errors(form.categorie)}}</td>
            </tr>
        </table>
        {{ form_end(form) }}
    </div>
{%endblock%}